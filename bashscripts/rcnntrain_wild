#!/bin/bash
#SBATCH --partition=A100medium
#SBATCH --nodes=1
#SBATCH --tasks-per-node=1
#SBATCH --job-name=test
#SBATCH --output=outputs/rcnn-wild-train-test-%j.out
#SBATCH --error=outputs/rcnn-wild-train-test_%j.err
#SBATCH --gres gpu:1
#SBATCH --cpus-per-task=1
#SBATCH --time=24:00:00
#SBATCH --mem=10gb
source /home/s6anthel/.bashrc
module load CUDA
conda activate kosmos2

epoch=$1
startepoch=$2
model=$3

echo "$epoch"
echo "$startepoch"
echo "$model"

python -m src.historicdocumentprocessing.fasterrcnn_trainer --name "testseveralcalls_4_with_valid_split" --epochs "$epoch" --startepoch "$startepoch" --load "$model"
